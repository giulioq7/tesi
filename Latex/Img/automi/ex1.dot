digraph G {
rankdir="LR"
imagescale=true
margin="0,0"
ratio=compress
node[fontsize=16,fixedsize=true,width=0.7,shape=circle,rank=min]
edge[fontsize=16]
maxiter=500
	start -> s0
	s0 -> s1 [label="a"]
	s1 -> s2 [label="b"]
	s0 -> s0 [label="b"]
	s1 -> s1 [label="a"]
	s2 -> s2 [label="a"]
	s2 -> s2 [label="b"]

	
	start [shape=point,style="invis",width=0]
	s0; s1;	s2 [shape=doublecircle];
}
